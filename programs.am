install-programs: $(PROGRAMS)
	$(top_srcdir)/mkinstalldirs $(bindir)
	for p in $(PROGRAMS); do \
	  $(INSTALL_PROGRAM) $$p $(bindir)/`echo $$p|sed '$(transform)'`; \
	done

uninstall-programs:
	for p in $(PROGRAMS); do \
	  rm -f $(bindir)/`echo $$p|sed '$(transform)'`; \
	done

mostlyclean-programs:

clean-programs:
	rm -f $(PROGRAMS)

distclean-programs:

maintainer-clean-programs:

