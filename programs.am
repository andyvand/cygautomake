install-@DIR@PROGRAMS: $(@DIR@_PROGRAMS)
	$(top_srcdir)/mkinstalldirs $(@DIR@dir)
	for p in $(@DIR@_PROGRAMS); do		\
	  if test -f $$p; then			\
	    $(INSTALL_PROGRAM) $$p $(@DIR@dir)/`echo $$p|sed '$(transform)'`; \
	  else :; fi				\
	done

uninstall-@DIR@PROGRAMS:
	for p in $(@DIR@_PROGRAMS); do \
	  rm -f $(@DIR@dir)/`echo $$p|sed '$(transform)'`; \
	done

