install-@DIR@LIBRARIES: $(@DIR@_LIBFILES)
	$(topsrcdir)/mkinstalldirs $(@DIR@dir)
	for p in $(@DIR@_LIBFILES); do		\
	  $(INSTALL_DATA) $$p $(@DIR@dir)/$$p;	\
## Must ranlib after installing because mod time changes.
	  $(RANLIB) $(@DIR@dir)/$$p;		\
	done

uninstall-@DIR@LIBRARIES:
	for p in $(@DIR@_LIBFILES); do		\
	  rm -f $(@DIR@dir)/$$p;		\
	done

