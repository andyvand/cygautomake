id:: ID

ID::
	here=`pwd`; cd $(srcdir) && mkid -f $$here/ID $(SOURCES)

tags:: TAGS

TAGS::
	here=`pwd`; cd $(srcdir) && etags $(SOURCES) -o $$here/TAGS

