mostlyclean:
	rm -f *.o core

clean: mostlyclean
	rm -f $(PROGRAMS) $(LIBPROGRAMS) $(LIBFILES) $(TEXFILES) $(CLEANFILES)

distclean: clean
	rm -f Makefile *.tab.c $(DISTCLEANFILES)
	rm -f Makefile config.cache config.log config.status
	rm -f ${CONFIG_HEADER} stamp-h

realclean: distclean
	rm -f TAGS

