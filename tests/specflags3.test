#! /bin/sh

# Test exe-specific flags with dependency tracking.

. $srcdir/defs || exit 1

cat >> configure.in << 'END'
AC_PROG_CC
END

cat > Makefile.am << 'END'
bin_PROGRAMS = foo
foo_SOURCES = foo.c
foo_CFLAGS = -DBAR
END

: > compile

$ACLOCAL || exit 1
$AUTOMAKE || exit 1

grep -e '-o foo-foo' Makefile.in
