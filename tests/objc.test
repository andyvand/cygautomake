#! /bin/sh

# Test to make sure LINK defined for Objective C.

. $srcdir/defs || exit 1

echo 'AC_SUBST(OBJC)' >> configure.in

cat > Makefile.am << 'END'
bin_PROGRAMS = foo
foo_SOURCES = foo.m

SUFFIXES = .m
.m.o:
	whatever
END

: > foo.m

$ACLOCAL || exit 1
$AUTOMAKE || exit 1

grep '^LINK' Makefile.in
