#! /bin/sh

# Make sure EXTRA_LIBRARIES aren't built by "all".
# Bug from Steve M Robbins.

. $srcdir/defs || exit 1

cat >> configure.in << 'END'
AC_PROG_CC
AC_PROG_RANLIB
END

cat > Makefile.am << 'END'
lib_LIBRARIES = libV.a
EXTRA_LIBRARIES = libVm.a
END

$AUTOMAKE || exit 1

(grep '^LIBFILES =' Makefile.in | grep libVm) && exit 1
exit 0
